{
	"manifest_version": 2,
	"name": "Trineo Fitbit link",
	"icons": { "128": "icon.png" },
	"version": "1.0",
	"description": "Adds link to Trineo group to header on fitbit.com.",
	"permissions": [
		"tabs",
		"https://code.jquery.com/jquery-2.1.1.min.js"
	],
	"content_scripts": [{
		"matches": ["https://www.fitbit.com/*"],
		"js": ["jquery-2.1.1.min.js", "contentScript.js"],
		"css": ["style.css"],
		"run_at": "document_end"
	}],
	"web_accessible_resources": ["trineo-logo.png"]
}
